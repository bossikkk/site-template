<%- include header %>

  <main>

    <div class="my-profile" uk-grid>

      <div class="uk-card uk-card-default uk-card-body uk-width-expand">
        <h3 class="uk-card-title uk-margin-remove-bottom">Profile</h3>
        <p>Your name is:
          <%= user.username %>
        </p>
				<p>Your email:
          <%= user.email %>
        </p>
        <p>Your subscribe:
          <%= user.subscribe %>
        </p>
        <p>Your Avatar url: <a target="_blank" href="<%= user.avatar %>"><%= user.avatar %></a></p>
      </div>

      <%- include sider %>
		</div>

  </main>

  <%- include footer %>